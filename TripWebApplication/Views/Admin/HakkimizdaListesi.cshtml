@using TripWebApplication.Models.Sınıflar
@model List<Hakkimizda>

@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Hakkımızda Listesi</h2>

<table class="table table-bordered">

    <tr>
        <th>ID</th>
        <th>PhotoUrl</th>
        <th>Açıklama</th>
        <th>Güncelle</th>
    </tr>
    @foreach (var x in Model)
    {
        <tr>
            <td>@x.ID</td>
            <td>@x.PhotoUrl</td>
            <td>@x.Aciklama</td>
            <td><a href="/Admin/HakkimizdaGetir/@x.ID" class="btn btn-warning">Güncelle</a></td>
        </tr>
    }


</table>


